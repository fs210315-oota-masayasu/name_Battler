<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.namebattler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".MainActivity">

           <intent-filter>
               <action android:name="android.intent.action.MAIN" />
               <category android:name="android.intent.category.LAUNCHER" />
           </intent-filter>

        </activity>

        <activity android:name=".characters.activity.CharacterListActivity">
        </activity>

        <activity android:name=".party.PartyFormationActivity">
        </activity>

        <activity android:name=".party.BattleLobbyActivity">
        </activity>

        <activity android:name=".characters.activity.NewCharacterGenerateActivity">
        </activity>

        <activity android:name=".characters.activity.ConfirmGenerationCharacterActivity">
        </activity>

        <activity android:name=".characters.activity.ConfirmCharacterStatusActivity">
        </activity>

        <activity android:name=".battle.activity.BattleMainActivity">
        </activity>

        <activity android:name=".battle.activity.OperationChangeActivity">
        </activity>



    </application>

</manifest>